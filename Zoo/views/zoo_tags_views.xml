<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="zoo_tags_action" model="ir.actions.act_window">
        <field name="name">Zoo tags</field>
        <field name="res_model">zoo.tags</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="zoo_tags_view_tree" model="ir.ui.view">
        <field name="name">zoo.tags.tree</field>
        <field name="model">zoo.tags</field>
        <field name="arch" type="xml">
            <tree string="Property Tags" editable="bottom" default_order="name">
               <field name="name"/> 
            </tree>
        </field>
    </record>
</odoo>